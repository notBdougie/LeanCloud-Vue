    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-node6": "^0.2.0",
    "babel-register": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "http-proxy-middleware": "^0.17.0",
    "jquery": "~3.0.0",
    "node-dev": "^3.1.3",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "pug-loader": "^1.0.2",
    "sass-loader": "^4.0.0",
    "semantic-ui": "^2.2.1",
    "sizzle": "^2.3.0",
